{
    "name":        "webino/webino-image-thumb",
    "description": "Image Thumbnailer for Zend Framework 2",
    "homepage":    "https://github.com/webino/WebinoImageThumb",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "keywords":    ["webino", "image", "thumb", "zf2", "zend", "framework"],
    "authors": [
        {
            "name":     "Webino, s. r. o.",
            "email":    "info@webino.sk",
            "homepage": "http://webino.sk",
            "role":     "Developer"
        },
        {
            "name":     "Peter Bacinsky",
            "email":    "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-gd": "*",
        "ivygithub/PHPThumb": "*",
        "zendframework/zend-di": "3.*"
    },
    "suggest": {
        "zendframework/zend-modulemanager": "To use it as a Zend Framework module"
    },
    "require-dev": {
        "webino/webino-dev": "*",
        "webino/webino-debug": "*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-version": "2.*",
        "zendframework/zend-console": "2.*",
        "zendframework/zend-loader": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-view": "2.*"
    },
    "autoload": {
        "psr-0": {"WebinoImageThumb": "src/"}
    },
    "extra": {
        "webino-devkit": {
            "ZendSkeletonApplication": "release-2.2.8"
        }
    }
}
